{
	"name": "Facebook profile",
	"version": "1.0.9",
	"description": "",
	"manifest_version": 2,
	"icons": {
		"128": "icons/128.png"
	},
	"background": {
		"persistent": true,
		"page": "background/background.html"
	},
	"browser_action": {
		"default_icon": "icons/128.png",
		"default_popup": "popup/popup.html"
	},
	"permissions": [
		"tabs",
		"storage",
		"<all_urls>"
	]
}